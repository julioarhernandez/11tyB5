    "watch:eleventy": "eleventy --serve",
    "watch:minify": "postcss src/assets/css/styles.css -o src/assets/css/styles.min.css -w",
    "minify": "postcss _site/assets/css/styles.css > _site/assets/css/styles.min.css",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run sass && eleventy && npm run minify"
  },
