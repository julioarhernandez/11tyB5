{
  "name": "11ty",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:parcel": "parcel watch ./src/_assets/images/* ./src/_assets/js/main.js --out-dir ./_site/assets",
    "dev:eleventy": "eleventy --serve",
    "sass": "sass --no-source-map src/_assets/css/custom.scss src/_assets/css/styles.css",
    "watch:sass": "npm run sass --no-source-map -- --watch",
    "watch:eleventy": "eleventy --serve",
    "minify": "postcss _site/assets/css/styles.css > _site/assets/css/styles.min.css",
    "start": "npm-run-all --parallel dev:*",
    "build": "npm run sass && eleventy && npm run minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.0.0-alpha1"
  },
  "devDependencies": {
    "postcss-scss": "^2.1.1",
    "@11ty/eleventy": "^0.11.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.6",
    "postcss-cli": "^7.1.2",
    "sass": "^1.26.10",
    "npm-run-all": "^4.1.5",
    "cssnano": "^4.1.10"
  }
}
